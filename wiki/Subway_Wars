		<a id="WikiaRandomWiki" href="http://community.wikia.com/wiki/Special:RandomWiki/8221" class="wikia-button">Random Wiki</a>
	</div>
	<script type='text/javascript'>
		wgAfterContentAndJS.push(function() {
			window.OpenXSPC = {};
			window.OpenXSPC[ 'fillElem_SPOTLIGHT_FOOTER' ] = function() {
				var output = window.OA_output || [];
				( 14 in output ) && $( '#SPOTLIGHT_FOOTER_1' ).html( output[ 14 ] );
